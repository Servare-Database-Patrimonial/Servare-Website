{
  "name": "servare-website",
  "version": "1.0.0",
  "description": "Página web corporativa de Servare - Database Patrimonial",
  "main": "index.html",
  "scripts": {
    "start": "live-server public --port=3001",
    "dev": "live-server public --port=3001 --watch=public --open=/",
    "build": "echo 'Website is static, no build needed'",
    "deploy:netlify": "netlify deploy --prod --dir=public",
    "deploy:vercel": "vercel --prod",
    "preview": "live-server public --port=8080 --no-browser"
  },
  "keywords": [
    "servare",
    "patrimonio",
    "conservación",
    "restauración",
    "arqueología",
    "museos"
  ],
  "author": "Servare Team",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.2"
  }
}
